func fatorial(x: number) -> number {
    if (x == 0 || x == 1) {
        return 1
    } else {
        return x * fatorial(x - 1)
    }
}

func fibonacci(n: number) -> number {
    if (n == 0) {
        return 0
    }
    if (n == 1) {
        return 1
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

par {
    print("fatorial de 5: ", fatorial(5))
    print("fib de 10: ", fibonacci(10))
}